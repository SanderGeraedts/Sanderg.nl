{
  "private": false,
  "name": "sanderg.nl-frontend",
  "version": "1.0.0",
  "author": "Sander Geraedts <info@sanderg.nl>",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "deploy": "npm run now-deploy",
    "build": "cross-env NODE_OPTIONS=\"-r esm\" gatsby build",
    "develop": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop",
    "empty-cache": "gatsby clean",
    "lint": "eslint .",
    "now-build": "npm run build",
    "workers-build": "npm run build",
    "workers-start": "npm run build && wrangler preview --watch",
    "workers-deploy": "npm run build && wrangler publish",
    "now-dev": "npm run build",
    "start": "npm run develop",
    "format": "prettier-eslint --write \"**/*.js\" \"!.cache/**\" \"!node_modules/**\" \"!public/**\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@cloudflare/wrangler": "^1.8.4",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/image-url": "^0.140.15",
    "date-fns": "^1.30.1",
    "dotenv": "^8.1.0",
    "gatsby": "^2.24.63",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.32",
    "gatsby-plugin-postcss": "^2.1.16",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sharp": "^2.6.37",
    "gatsby-plugin-styled-components": "^3.3.12",
    "gatsby-source-filesystem": "^2.3.31",
    "gatsby-source-sanity": "^5.0.4",
    "gatsby-transformer-sharp": "^2.5.16",
    "normalize.css": "^8.0.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.11.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "prop-types": "^15.7.2",
    "cross-env": "^7.0.2",
    "esm": "^3.2.25"
  }
}
